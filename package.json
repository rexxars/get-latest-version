{
  "name": "get-latest-version",
  "version": "5.1.0",
  "description": "Find the latest version of an npm module matching a given semver range",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "test": "node --test test/test.js",
    "posttest": "eslint ."
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rexxars/get-latest-version.git"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "npm",
    "registry",
    "version",
    "semver"
  ],
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rexxars/get-latest-version/issues"
  },
  "homepage": "https://github.com/rexxars/get-latest-version#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@sanity/semantic-release-preset": "^6.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.0.0",
    "semantic-release": "^25.0.3"
  },
  "dependencies": {
    "get-it": "^8.7.0",
    "registry-auth-token": "^5.1.1",
    "registry-url": "^7.2.0",
    "semver": "^7.7.4"
  }
}
